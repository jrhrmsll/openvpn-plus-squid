iptables -t nat -A PREROUTING -p tcp --dport 80 -i tun0 -j DNAT --to 192.168.10.5:3128
iptables -t nat -A PREROUTING -p tcp --dport 443 -i tun0 -j DNAT --to 192.168.10.5:3128
iptables -t nat -A POSTROUTING -s 10.8.0.0/24 -o enp0s8 -j MASQUERADE

route add -net 172.16.0.0/24 gw 192.168.10.100 enp0s8
route add -net 192.168.10.0/24 gw 172.16.0.100 enp0s8
